{
  "name": "permit-school-web",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:offline": "OFFLINE_DEV=1 next dev -p 3000",
    "build": "next build",
    "build:ci": "next build",
    "start": "next start -p 3000",
    "start:offline": "OFFLINE_DEV=1 next start -p 3000",
    "start-ci": "next start -p 3000",
    "start:ci": "node .next/standalone/server.js -p ${PORT:-4330}",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0",
    "env:copy": "node -e \"const fs=require('fs'); if(!fs.existsSync('.env.local')) fs.copyFileSync('.env.local.example','.env.local'); if(!fs.existsSync('.env.development')) fs.copyFileSync('.env.development.example','.env.development'); if(!fs.existsSync('.env.production')) fs.copyFileSync('.env.production.example','.env.production')\"",
    "env:seed": "node ../scripts/seed-env.mjs",
    "env:seed:auto": "node ../scripts/seed-env.mjs --non-interactive",
    "lint:a11y": "eslint src --ext .tsx --plugin jsx-a11y --rule 'jsx-a11y/alt-text:error' --rule 'jsx-a11y/anchor-has-content:error' --rule 'jsx-a11y/anchor-is-valid:error' --rule 'jsx-a11y/click-events-have-key-events:error' --rule 'jsx-a11y/heading-has-content:error' --rule 'jsx-a11y/html-has-lang:error' --rule 'jsx-a11y/iframe-has-title:error' --rule 'jsx-a11y/img-redundant-alt:error' --rule 'jsx-a11y/no-access-key:error' --rule 'jsx-a11y/no-autofocus:error' --rule 'jsx-a11y/no-distracting-elements:error' --rule 'jsx-a11y/no-interactive-element-to-noninteractive-role:error' --rule 'jsx-a11y/no-noninteractive-element-interactions:error' --rule 'jsx-a11y/no-noninteractive-tabindex:error' --rule 'jsx-a11y/no-redundant-roles:error' --rule 'jsx-a11y/no-static-element-interactions:error' --rule 'jsx-a11y/role-has-required-aria-props:error' --rule 'jsx-a11y/role-supports-aria-props:error' --rule 'jsx-a11y/scope:error' --rule 'jsx-a11y/tabindex-no-positive:error' --max-warnings=0",
    "typecheck": "tsc --noEmit -p tsconfig.build.json",
    "test": "vitest run",
    "test:watch": "vitest",
    "axe:ci": "axe http://localhost:4330/ http://localhost:4330/courses http://localhost:4330/signin --exit 1 || true",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:install": "playwright install",
    "seed:handbooks": "tsx ops/seed/seed-handbooks.ts",
    "lhci": "lhci autorun",
    "env:check": "tsx -e \"import('./src/env.ts').then(()=>console.log('web env ok'))\""
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^6.0.0",
    "@mui/material": "^6.0.0",
    "@mui/material-nextjs": "^6.0.0",
    "@mui/x-date-pickers": "^8.10.0",
    "@sentry/nextjs": "^10.4.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-helpers-react": "^0.4.0",
    "@supabase/supabase-js": "^2.45.4",
    "@types/jszip": "^3.4.0",
    "archiver": "^7.0.1",
    "date-fns": "^4.1.0",
    "jszip": "^3.10.1",
    "next": "14.2.7",
    "next-intl": "4.3.4",
    "next-pwa": "^5.6.0",
    "otplib": "^12.0.1",
    "pdf-lib": "^1.17.1",
    "qrcode": "^1.5.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "resend": "^6.0.1",
    "stripe": "^18.4.0",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@axe-core/cli": "^4.8.4",
    "@axe-core/playwright": "^4.10.2",
    "@playwright/test": "^1.42.1",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/archiver": "^6.0.3",
    "@types/node": "^20.14.12",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitest/coverage-v8": "^2.0.5",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.7",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-testing-library": "^6.2.3",
    "eslint-plugin-vitest": "^0.5.4",
    "jsdom": "^26.1.0",
    "lighthouse": "^11.6.0",
    "tsx": "^4.19.2",
    "typescript": "^5.5.4",
    "vite-tsconfig-paths": "^5.1.2",
    "vitest": "^2.0.5",
    "zod": "^4.0.17"
  }
}
