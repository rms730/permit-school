    "seed:questions:unit1": "tsx ops/seed/seed-questions.ts",
    "seed:blueprint:unit1": "tsx ops/seed/seed-blueprint.ts",
    "seed:verify": "tsx ops/seed/verify-seed.ts",
    "seed:unit2": "tsx ops/seed/seed-unit.ts --unit=02 --lang=en,es --j=CA --course=DE-ONLINE",
