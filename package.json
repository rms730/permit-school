{
  "name": "permit-school",
  "private": true,
  "scripts": {
    "docs:inventory": "node tools/docs/inventory.mjs",
    "docs:validate-commands": "node tools/docs/validate-commands.mjs",
    "docs:lint": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\" --ignore \"**/web/playwright-report/**\" --ignore \"**/web/test-results/**\" --ignore \"**/artifacts/**\"",
    "docs:links": "find . -name \"*.md\" -not -path \"./node_modules/*\" -not -path \"./web/node_modules/*\" -not -path \"./web/playwright-report/*\" -not -path \"./web/test-results/*\" -not -path \"./artifacts/*\" -not -path \"./tools/ingest/*\" | xargs markdown-link-check -q -p -c tools/docs/mlc.config.json",
    "docs:check": "npm run docs:inventory && npm run docs:lint && npm run docs:links",
    "env:copy:root": "node -e \"const fs=require('fs'); if(!fs.existsSync('.env.local')) fs.copyFileSync('env-examples/root.env.local.example','.env.local'); if(!fs.existsSync('.env.dev')) fs.copyFileSync('env-examples/root.env.dev.example','.env.dev'); if(!fs.existsSync('.env.prod')) fs.copyFileSync('env-examples/root.env.prod.example','.env.prod')\"",
    "env:copy:web": "node -e \"const fs=require('fs'); if(!fs.existsSync('web/.env.local')) fs.copyFileSync('env-examples/web.env.local.example','web/.env.local'); if(!fs.existsSync('web/.env.development')) fs.copyFileSync('env-examples/web.env.development.example','web/.env.development'); if(!fs.existsSync('web/.env.production')) fs.copyFileSync('env-examples/web.env.production.example','web/.env.production')\"",
    "env:copy": "npm run env:copy:root && npm run env:copy:web",
    "env:seed": "node scripts/seed-env.mjs --env local",
    "env:seed:all": "node scripts/seed-env.mjs --env all",
    "env:seed:dry": "node scripts/seed-env.mjs --env all --dry-run",
    "env:seed:prod": "node scripts/seed-env.mjs --env prod --non-interactive",
    "env:check:local": "npx tsx tools/env/validate.ts local",
    "env:check:dev": "npx tsx tools/env/validate.ts development",
    "env:check:prod": "npx tsx tools/env/validate.ts production",
    "format:check": "npx prettier@3.3.3 -c \"**/*.{md,mdx,yml,yaml,json}\"",
    "format:fix": "npx prettier@3.3.3 --write \"**/*.{md,mdx,yml,yaml,json}\"",
    "ci:check": "npm run format:check && npm --prefix web run lint && npm --prefix web run typecheck",
    "env:setup": "node tools/env/setup.mjs",
    "env:setup:dry": "node tools/env/setup.mjs",
    "ci:check:full": "npm run format:check && npm run env:check:dev && npm --prefix web run lint && npm --prefix web run typecheck",
    "seed:normalize": "tsx ops/seed/normalize/cli.ts",
    "seed:normalize:write": "tsx ops/seed/normalize/cli.ts --write",
    "seed:normalize:curr": "tsx ops/seed/normalize/cli.ts --mode=curriculum",
    "seed:normalize:curr:write": "tsx ops/seed/normalize/cli.ts --mode=curriculum --write",
    "seed:normalize:questions": "tsx ops/seed/normalize/cli.ts --mode=questions",
    "seed:normalize:questions:write": "tsx ops/seed/normalize/cli.ts --mode=questions --write",
    "seed:verify:normalize": "tsx ops/seed/normalize/verify.ts",
    "seed:final": "tsx ops/seed/seed-final-exam.ts",
    "seed:final:verify": "tsx ops/seed/verify-final-exam.ts",
    "seed:unit1": "tsx ops/seed/seed-unit.ts",
    "seed:curriculum:unit1": "tsx ops/seed/seed-curriculum.ts",
    "seed:questions:unit1": "tsx ops/seed/seed-questions.ts",
    "seed:blueprint:unit1": "tsx ops/seed/seed-blueprint.ts",
    "seed:verify": "tsx ops/seed/verify-seed.ts",
    "seed:unit2": "tsx ops/seed/seed-unit.ts --unit=02 --lang=en,es --j=CA --course=DE-ONLINE",
    "seed:curriculum:unit2": "tsx ops/seed/seed-curriculum.ts --unit=02 --lang=en,es --j=CA --course=DE-ONLINE",
    "seed:questions:unit2": "tsx ops/seed/seed-questions.ts --unit=02 --lang=en,es --j=CA --course=DE-ONLINE",
    "seed:blueprint:unit2": "tsx ops/seed/seed-blueprint.ts --unit=02 --j=CA --course=DE-ONLINE",
    "seed:verify:unit2": "tsx ops/seed/verify-seed.ts --unit=02 --lang=en,es --j=CA --course=DE-ONLINE",
    "seed:unit3": "tsx ops/seed/seed-unit.ts --unit=03 --lang=en,es --j=CA --course=DE-ONLINE",
    "seed:unit4": "tsx ops/seed/seed-unit.ts --unit=04 --lang=en,es --j=CA --course=DE-ONLINE",
    "seed:unit5": "tsx ops/seed/seed-unit.ts --unit=05 --lang=en,es --j=CA --course=DE-ONLINE",
    "seed:unit6": "tsx ops/seed/seed-unit.ts --unit=06 --lang=en,es --j=CA --course=DE-ONLINE",
    "seed:unit7": "tsx ops/seed/seed-unit.ts --unit=07 --lang=en,es --j=CA --course=DE-ONLINE",
    "seed:unit8": "tsx ops/seed/seed-unit.ts --unit=08 --lang=en,es --j=CA --course=DE-ONLINE",
    "seed:unit9": "tsx ops/seed/seed-unit.ts --unit=09 --lang=en,es --j=CA --course=DE-ONLINE",
    "seed:unit10": "tsx ops/seed/seed-unit.ts --unit=10 --lang=en,es --j=CA --course=DE-ONLINE",
    "seed:unit11": "tsx ops/seed/seed-unit.ts --unit=11 --lang=en,es --j=CA --course=DE-ONLINE",
    "seed:unit12": "tsx ops/seed/seed-unit.ts --unit=12 --lang=en,es --j=CA --course=DE-ONLINE",
    "seed:verify:unit3": "tsx ops/seed/verify-seed.ts --unit=03 --lang=en,es --j=CA --course=DE-ONLINE",
    "seed:verify:unit4": "tsx ops/seed/verify-seed.ts --unit=04 --lang=en,es --j=CA --course=DE-ONLINE",
    "seed:verify:unit5": "tsx ops/seed/verify-seed.ts --unit=05 --lang=en,es --j=CA --course=DE-ONLINE",
    "seed:verify:unit6": "tsx ops/seed/verify-seed.ts --unit=06 --lang=en,es --j=CA --course=DE-ONLINE",
    "seed:verify:unit7": "tsx ops/seed/verify-seed.ts --unit=07 --lang=en,es --j=CA --course=DE-ONLINE",
    "seed:verify:unit8": "tsx ops/seed/verify-seed.ts --unit=08 --lang=en,es --j=CA --course=DE-ONLINE",
    "seed:verify:unit9": "tsx ops/seed/verify-seed.ts --unit=09 --lang=en,es --j=CA --course=DE-ONLINE",
    "seed:verify:unit10": "tsx ops/seed/verify-seed.ts --unit=10 --lang=en,es --j=CA --course=DE-ONLINE",
    "seed:verify:unit11": "tsx ops/seed/verify-seed.ts --unit=11 --lang=en,es --j=CA --course=DE-ONLINE",
    "seed:verify:unit12": "tsx ops/seed/verify-seed.ts --unit=12 --lang=en,es --j=CA --course=DE-ONLINE",
    "seed:make-empty-units": "node ops/seed/scripts/make-empty-units.mjs",
    "seed:all": "tsx ops/seed/seed-all.ts",
    "seed:curriculum": "tsx ops/seed/seed-curriculum.ts",
    "seed:questions": "tsx ops/seed/seed-questions.ts",
    "icons:make-favicon": "node tools/images/make-favicon.mjs",
    "reports:open": "node tools/report/open-report.mjs",
    "reports:serve": "node tools/report/serve-report.mjs --dir web/playwright-report --port 9323 --timeout 20000",
    "reports:ci": "node tools/report/serve-report.mjs --dir web/playwright-report --ci --no-open --timeout 0",
    "ci:local": "bash scripts/ci-local.sh",
    "ci:env": "node tools/env/ci-setup.mjs",
    "ci:server": "npm run -w web start:ci",
    "ci:wait": "npx wait-on -t 120000 http-get://localhost:4330/en",
    "ci:lint": "npm run -w web lint",
    "ci:test": "npm run -w web test -- --run",
    "ci:e2e": "npm run -w web test:e2e",
    "ci:lh": "npx lhci autorun --config=web/lhci.config.js",
    "ci:all": "npm ci && npm --workspace web ci && npm run ci:env && (npm run ci:server &) && npm run ci:wait && npm run ci:lint && npm run ci:test && npm run ci:e2e && npm run ci:lh",
    "prepare": "husky"
  },
  "devDependencies": {
    "@supabase/supabase-js": "^2.47.0",
    "dotenv-flow": "^4.1.0",
    "get-port": "^7.1.0",
    "globby": "^14.0.1",
    "gray-matter": "^4.0.3",
    "husky": "^9.1.7",
    "kill-port": "^2.0.1",
    "markdown-link-check": "^3.12.2",
    "markdownlint-cli": "^0.41.0",
    "open": "^10.2.0",
    "to-ico": "^1.1.5",
    "tree-kill": "^1.2.2",
    "tsx": "^4.15.7",
    "zod": "^4.0.17"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "uuid": "^11.1.0"
  }
}
