# -------------------------------------------------------------------
# WEB .env.production.example  (Next.js production environment)
# -------------------------------------------------------------------
# Copy to web/.env.production and fill in secrets.
# This file is read by the Next.js application in production.
# -------------------------------------------------------------------

# --- Supabase (Client-safe) ---
NEXT_PUBLIC_SUPABASE_URL=https://your-prod-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# --- Stripe (Client-safe) ---
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_live_your_publishable_key_here

# --- Monitoring (Sentry) ---
NEXT_PUBLIC_SENTRY_DSN=https://your_prod_public_sentry_dsn_here

# --- Environment ---
NEXT_PUBLIC_ENV=production
NEXT_PUBLIC_COMMIT_SHA=prod
NEXT_PUBLIC_BUILD_AT=

# --- Site Configuration ---
NEXT_PUBLIC_SITE_URL=https://your-production-domain.com

# --- Google One Tap (optional) ---
NEXT_PUBLIC_GOOGLE_ONE_TAP=1
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_production_google_client_id_here

# --- Support Information ---
NEXT_PUBLIC_SUPPORT_EMAIL=support@your-production-domain.com
NEXT_PUBLIC_SUPPORT_PHONE=+1-555-0123



# --- Additional Client-Safe Variables ---
NEXT_PUBLIC_APP_BASE_URL=https://your-production-domain.com
NEXT_PUBLIC_VAR=prod_value

# --- Additional Server-Only Variables (for Next.js API routes) ---
NODE_ENV=production
RATE_LIMIT_ON=true
RATE_LIMIT_WINDOW_MS=60000
REGULATORY_MONTHLY_ENABLED=true
STRIPE_PRICE_ID=price_your_live_price_id_here
VAR=prod_value
