# -------------------------------------------------------------------
# WEB .env.development.example  (Next.js development environment)
# -------------------------------------------------------------------
# Copy to web/.env.development and fill in secrets.
# This file is read by the Next.js application in development.
# -------------------------------------------------------------------

# --- Supabase (Client-safe) ---
NEXT_PUBLIC_SUPABASE_URL=https://your-dev-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# --- Stripe (Client-safe) ---
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key_here

# --- Monitoring (Sentry) ---
NEXT_PUBLIC_SENTRY_DSN=https://your_dev_public_sentry_dsn_here

# --- Environment ---
NEXT_PUBLIC_ENV=development
NEXT_PUBLIC_COMMIT_SHA=dev
NEXT_PUBLIC_BUILD_AT=

# --- Site Configuration ---
NEXT_PUBLIC_SITE_URL=https://your-dev-domain.com

# --- Google One Tap (optional) ---
NEXT_PUBLIC_GOOGLE_ONE_TAP=0
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id_here

# --- Support Information ---
NEXT_PUBLIC_SUPPORT_EMAIL=support@your-dev-domain.com
NEXT_PUBLIC_SUPPORT_PHONE=+1-555-0123

# --- Admin Configuration ---
# This is now managed in the database via jurisdiction_configs table
# NEXT_PUBLIC_FULFILLMENT_LOW_STOCK_THRESHOLD=200

# --- Additional Client-Safe Variables ---
NEXT_PUBLIC_APP_BASE_URL=https://your-dev-domain.com
NEXT_PUBLIC_VAR=dev_value

# --- Additional Server-Only Variables (for Next.js API routes) ---
NODE_ENV=development
RATE_LIMIT_ON=true
RATE_LIMIT_WINDOW_MS=60000
REGULATORY_MONTHLY_ENABLED=true
STRIPE_PRICE_ID=price_your_test_price_id_here
VAR=dev_value
